{
    "name": "pderas/chronicle",
    "description": "A note sharing library that lets users interact with each other using file uploads and comments.",
    "keywords": ["notes", "files", "comments", "chronicle", "laravel"],
    "homepage": "https://https://github.com/PDERAS/chronicle",
    "type": "library",
    "license": "ISC",
    "authors": [
        {
            "name": "Cody Moorhouse",
            "email": "cody.moorhouse@icloud.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Pderas\\Chronicle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        },
        "laravel": {
            "providers": [
                "Pderas\\Chronicle\\ChronicleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
